<form id="saveBulkAssetMaintenance" enctype="multipart/form-data" class="form-horizontal auth-form" method="post" autocomplete="off">
    <div class="card card-one">
        <ul>
            @if (ViewBag.Error is not null)
            {
                @foreach (var error in ViewBag.Error)
                {
                    <li class="text-danger">@error</li>
                }
            }
        </ul>
        <div class="card-body p-3">
            <div class="row">
                <div class="col">
                    <label for="formFile" class="form-label">Upload File</label>
                    <input class="form-control" type="file" id="excelFile" name="excelFile" accept=".xlsx">

                </div>

            </div>
        </div>
        <div class="modal-footer mt-3">
            <button type="button" class="btn btn-dark waves-effect" data-bs-dismiss="modal">Cancel</button>
            <button class="btn btn-primary" type="submit">Submit <i class="ri-send-plane-fill"></i></button>
        </div>
        <!-- card-body -->
    </div><!-- card -->
</form>
<script>

    FormHandler.init({
        method: 'POST',
        actionUrl: '@Url.Action("UploadBulkExcel", "AssetMaintenance")',
        containerSelector: '#add-new .modal-body',
        selector: '#saveBulkAssetMaintenance'
    }, function () {
        $("#add-new .modal-body").html("");
        $("#add-new .modal-title").html("");
        $("#add-new").modal('hide');

        Pagination.getData();
    });
</script>